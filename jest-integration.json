{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "roots": ["<rootDir>/src/"],
  "testEnvironment": "node",
  "testRegex": ".integration-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFilesAfterEnv": [
    "jest-extended",
    "<rootDir>/setupJestMock.ts",
    "<rootDir>/jest-integration-custom-matchers.ts"
  ]
}
